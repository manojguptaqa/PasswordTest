    "expectedResult": "Invalid",
    "expectedMessage": "Invalid:Password Length is 17"
  },
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "aGODA@123456aGODA",
    "oldPasswordMatched": true,
    "expectedResult": "Invalid",
    "expectedMessage": "Invalid:Password Length is 17"
  },
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "agoda@123456agodaa",
