[
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "Agoda@123456AgodaA",
    "oldPasswordMatched": true,
    "expectedResult": "Valid",
    "expectedMessage": "Valid:Password Length is 18"
  },
  {
    "oldPassword": "Agoda@123456AgodaAAAAA",
    "newPassword": "Agoda@123456AgodaAAAAA",
    "oldPasswordMatched": true,
    "expectedResult": "Invalid",
    "expectedMessage": "Invalid:Password same as old password."
  },
  {
    "oldPassword": "Agoda@123456AgodaA",
    "newPassword": "Agoda@654892AgodaA",
    "oldPasswordMatched": true,
    "expectedResult": "Valid",
    "expectedMessage": "Valid:Password changed."
  },
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "aGODA@123456aGODAa",
    "oldPasswordMatched": true,
    "expectedResult": "Valid",
    "expectedMessage": "Valid:Password Length is 18"
  },
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "aGODA@123456aGODAaaaaa",
    "oldPasswordMatched": false,
    "expectedResult": "Invalid",
    "expectedMessage": "Invalid:Old password did not match with system."
  },
  {
    "oldPassword": "Agoda@987376AgodaA",
    "newPassword": "Agoda@987376AgodaA",
